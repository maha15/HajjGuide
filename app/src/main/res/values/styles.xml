<resources>

    <!-- Base application theme. -->
    <style title="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item title="colorPrimary">@color/colorPrimary</item>
        <item title="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item title="colorAccent">@color/colorAccent</item>
    </style>

    <style title="AppTheme.NoActionBar">
        <item title="windowActionBar">false</item>
        <item title="windowNoTitle">true</item>
    </style>

    <style title="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style title="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>
